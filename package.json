{
  "name": "rite-monorepo",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:next": "turbo run dev --filter=next-app",
    "dev:mobile": "turbo run start --filter=mobile -- -c",
    "dev:backend": "turbo run dev --filter=@rite/backend",
    "predev": "pnpm --filter=@rite/backend run dev --until-success && pnpm --filter=@rite/backend convex dashboard",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@tamagui/babel-plugin": "^1.132.15",
    "@tamagui/next-plugin": "^1.132.15",
    "@testing-library/jest-dom": "^6.6.4",
    "@vitest/ui": "^3.2.4",
    "c8": "^10.1.3",
    "happy-dom": "^18.0.1",
    "msw": "^2.10.4",
    "turbo": "^2.5.0",
    "vitest": "^3.2.4",
    "vite": "^5.4.9"
  },
  "dependencies": {
    "@tamagui/animations-react-native": "^1.132.15",
    "@tamagui/button": "^1.132.15",
    "@tamagui/config": "^1.132.15",
    "@tamagui/core": "^1.132.15",
    "@tamagui/font-inter": "^1.132.15",
    "@tamagui/next-theme": "^1.132.16",
    "@tamagui/stacks": "^1.132.15",
    "@tamagui/text": "^1.132.15",
    "@tamagui/themes": "^1.132.15",
    "next-themes": "^0.4.6"
  }
}
